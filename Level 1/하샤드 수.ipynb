    "def solution(x):\n",
    "    \n",
    "    lst = list(map(int, str(x)))\n",
    "    \n",
    "    s = sum(lst)\n",
    "    \n",
    "    if x % s == 0:\n",
    "        answer = True\n",
    "    else:\n",
    "        answer = False\n",
    "    return answer"
